/*
Enter your query here.
Please append a semicolon ";" at the end of the query and enter your query in a single line to avoid error.
*/

declare @n int;
declare @counter int;

set @n = 20;
set @counter = -1;

while @counter <= @n
begin
    if @counter < 22
    begin
        print replicate("* ", @counter);
    end
    else
    begin
        print "* * * * * * * * * * * * * * * * * * * *";
    end
    set @counter += 1;
end
